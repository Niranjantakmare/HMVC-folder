     border-bottom: 1px solid #ccc;
}
 .runningboder ~ .focus-border{
     position: absolute;
     bottom: 0;
     left: 10%;
     width: 0;
     height: 2px;
     background-color: #4b2d8a;
     transition: 0.4s;
}
 .runningboder:focus ~ .focus-border{
     width: 80%;
     transition: 0.4s;
}
