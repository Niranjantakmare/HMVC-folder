border: none; 
color: #390858 !important; 
background: transparent;
font-weight:600;
}
.nav-tabs > li > a::after { 
content: ""; 
background: #837f1b; 
height: 4px; 
position: absolute; 
width: 100%; 
left: 0px; 
bottom: -1px; 
transition: all 250ms ease 0s; 
transform: scale(0); 
}
.nav-tabs > li.active > a::after, .nav-tabs > li:hover > a::after {
transform: scale(1); 
}
.tab-nav > li > a::after { 
